{
    "$schema": "https://raw.githubusercontent.com/sivrad/matrix-schema/main/type.json",
    "name": "Entity",
    "label": "Entity",
    "description": "An entity is a physical thing that can or has existed.",
    "icon": "https://static.thenounproject.com/png/6887-200.png",
    "fields": {
        "end": {
            "label": "End",
            "type": "Time | null",
            "description": "A time when the thing stopped existing.",
            "defaultValue": null
        },
        "height": {
            "label": "Height",
            "type": "quantity.Length | null",
            "description": "The height of the entity.",
            "defaultValue": null
        },
        "mass": {
            "label": "Mass",
            "type": "quantity.Mass | null",
            "description": "The mass of the entity.",
            "defaultValue": null
        }
    }
}
